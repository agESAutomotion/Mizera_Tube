 CASE 540.7   MSGOUT VA1 ErrMsg "C1 SLAVE (FISSO) NON APERTO E BLOCCATO"

 CASE 540.21  MSGOUT VA1 ErrMsg "PREDISPONI ASSE U E C"
 CASE 540.22  MSGOUT VA1 ErrMsg "PREDISPONI SCARICO"
 CASE 540.23  MSGOUT VA1 ErrMsg "PRESA TUBO"

 CASE 550.1   MSGOUT VA1 ErrMsg "JAW-REG MINIMA H=" VA2 " W=" VA3 "
 CASE 550.2   MSGOUT VA1 ErrMsg "REGOLAZIONE MANUALE JAW-REG FUORI RANGE Reg=" VA2
 CASE 550.11  MSGOUT VA1 ErrMsg "GRUPPO JAWREG NON IN POSIZIONE DI REGOLAZIONE"
 CASE 550.12  MSGOUT VA1 ErrMsg "Cm MOBILE NON IN POSIZIONE DI REGOLAZIONE"
 CASE 550.13  MSGOUT VA1 ErrMsg "Cs FISSO NON IN POSIZIONE DI REGOLAZIONE"
 CASE 550.15  MSGOUT VA1 ErrMsg "JAWREG RAGGIUNTO MASSIMO NUMERO DI TENTATIVI"
 CASE 550.16  MSGOUT VA1 ErrMsg "JAWREG LIMITE MINIMO NEGATIVO"

 CASE 570.1   MSGOUT VA1 ErrMsg "TUBO NON PRESENTE FC TUBOPEZZO"
 CASE 570.2   MSGOUT VA1 ErrMsg "C MASTER (MOBILE) NON CHIUSO E SBLOCCATO"
 CASE 570.3   MSGOUT VA1 ErrMsg "C1 SLAVE (FISSO) NON CHIUSO E SBLOCCATO"

 CASE 570.21  MSGOUT VA1 ErrMsg "START ZERO TUBO"

 CASE 580.1   MSGOUT VA1 ErrMsg "DATI TUBO H=" VA2 " W=" VA3 " R=" VA4
 CASE 580.2   MSGOUT VA1 ErrMsg "TIPO TUBO NON GESTITO T=" VA2
 CASE 580.3   MSGOUT VA1 ErrMsg "CALCOLO DIAGONALE FUORI RANGE D=" VA2
 CASE 580.4   MSGOUT VA1 ErrMsg "C MASTER (MOBILE) NON CHIUSO E SBLOCCATO"
 CASE 580.5   MSGOUT VA1 ErrMsg "C1 SLAVE (FISSO) NON CHIUSO E SBLOCCATO"
 CASE 580.6   MSGOUT VA1 ErrMsg "TUBO NON SCARICATO FC PRESENZA TUBO/PEZZO"
 CASE 580.7   MSGOUT VA1 ErrMsg "MEMORIA PRESENTA TUBO ATTIVA"
 CASE 580.8   MSGOUT VA1 ErrMsg "MANDRINI DISALINEATI DifPos>" VA2
 
 CASE 590.1   MSGOUT VA1 ErrMsg "C MASTER (MOBILE) NON CHIUSO E SBLOCCATO"
 CASE 590.2   MSGOUT VA1 ErrMsg "C1 SLAVE (FISSO) NON CHIUSO E SBLOCCATO"
 CASE 590.3   MSGOUT VA1 ErrMsg "MANDRINI DISALINEATI DifPos>" VA2

 OTHERWISE
   MSGOUT VA1 ErrMsg "ERRORE NON GESTITO"
ENDSWITCH


RET