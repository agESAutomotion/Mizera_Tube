 [L]0 : G740

;// Termina una sezione bevel.
;// Vedi [Sintassi Cicli Opzione Bevel]
;//
;// G740
;//

IF( gAttachState != 2 ) THEN
    ERROR(3)    ;//Funzione non valida
ENDIF

G840

IF(!VA11)THEN
    G153 G0 Z(trav_safe_z)
ELSE
    IF( kf_HeadIsAB7 ) THEN
        G180 X0 Y0 Z(0.0+workpiece_safe_dist_leave) A0 B0
    ELSE
        G180 X0 Y0 Z(0.0+workpiece_safe_dist_leave) C0 A0
    ENDIF
    IF( gTechType != 2 ) G153 G0 Z(kine_z+head_safe_dist)
ENDIF

gAttachState = 0

RET

