<?xml version="1.0" encoding="iso-8859-1"?>
<!--Danaher drive configured for Cyclic Synchronous Position Mode-->
<Config DeviceConfig="s400s600_9.xml">
	<SetUpDrive>
		<ProgramListing><![CDATA[
		]]></ProgramListing>
	</SetUpDrive>

    <Comment><![CDATA[FMMU and Sync Managment tables]]></Comment>
	<FMMUTable LStart="#x12000" Length="0" LStartBit="0" LEndBit="7" PStart="#x1100">0</FMMUTable>
	<FMMUTable LStart="#x11000" Length="0" LStartBit="0" LEndBit="7" PStart="#x1140">1</FMMUTable>
	<FMMUTable LStart="#x0" Length="0" LStartBit="0" LEndBit="0" PStart="#x0">2</FMMUTable>
	<FMMUTable LStart="#x0" Length="0" LStartBit="0" LEndBit="0" PStart="#x0">3</FMMUTable>

	<SYNCTable Start="#x1800" Length="512" Type="MBoxOutput" Data="#x26000100">0</SYNCTable>
	<SYNCTable Start="#x1C00" Length="512" Type="MBoxInput" Data="#x22000100">1</SYNCTable>
	<SYNCTable Start="#x1100" Length="0" Type="Output" Data="#x24000100">2</SYNCTable>
	<SYNCTable Start="#x1140" Length="0" Type="Input" Data="#x22000100">3</SYNCTable>
    <Comment><![CDATA[FMMU and Sync Managment tables]]></Comment>


    <MboxPolling>
      <Comment><![CDATA[0=Cyclic 1=State Change]]></Comment>
      <Type>0</Type>
    </MboxPolling>
    
	<DownloadPDOAssignment>1</DownloadPDOAssignment>
    <DownloadPDOConfiguration>0</DownloadPDOConfiguration>

	<PDOAssignmentSM2 Output="#x1701">
		<SMObject>
		  <Comment><![CDATA[Position demand value]]></Comment>
		  <Address>#x60620020</Address>
		</SMObject>
		<SMObject>
		  <Comment><![CDATA[Ctrl Word]]></Comment>
		  <Address>#x60400010</Address>
		</SMObject>
	</PDOAssignmentSM2>
	
	<PDOAssignmentSM3 Input="#x1B01">
		<SMObject>
		  <Comment><![CDATA[Position actual value]]></Comment>
		  <Address>#x60640020</Address>
		</SMObject>
		<SMObject>
		  <Comment><![CDATA[Status Word]]></Comment>
		  <Address>#x60410010</Address>
		</SMObject>
	</PDOAssignmentSM3>

	<DistribuitedClock>1</DistribuitedClock>

	<CyclicOperation>
		<Enabled>1</Enabled>

		<Sync0>1</Sync0>
		<Sync0CycleTime AdaptCycleTime="1">2000</Sync0CycleTime>
		<Sync0ShiftTime>0</Sync0ShiftTime>

		<Sync1>1</Sync1>
		<Sync1CycleTime AdaptCycleTime="1">2000</Sync1CycleTime>
		<Sync1ShiftTime>0</Sync1ShiftTime>
	</CyclicOperation>

</Config>


