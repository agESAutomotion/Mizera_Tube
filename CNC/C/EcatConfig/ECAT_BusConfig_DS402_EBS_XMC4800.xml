<?xml version="1.0" encoding="iso-8859-1"?>
<!--ByStorm drive configured for Profile Velocity Mode-->
<Config DeviceConfig="XMC4800_BySTORM.xml">

    <Device>XMC4800_TEST</Device>
    
    <Slot></Slot>
    
    <Module></Module>

    <RxPdo Index="#x1600">
		<SMObject>
		  <Comment><![CDATA[Ctrl Word]]></Comment>
		  <Address>#x60400010</Address>
		</SMObject>
		<SMObject>
		  <Comment><![CDATA[Ctrl Word 2]]></Comment>
		  <Address>#x300F0010</Address>
		</SMObject>
		<SMObject>
		  <Comment><![CDATA[Target Position]]></Comment>
		  <Address>#x60C10120</Address>
		</SMObject>
		<SMObject>
		  <Comment><![CDATA[Target Velocity]]></Comment>
		  <Address>#x60FF0020</Address>
		</SMObject>
	</RxPdo>
	
	<TxPdo Index="#x1A00">
		<SMObject>
		  <Comment><![CDATA[Status Word]]></Comment>
		  <Address>#x60410010</Address>
		</SMObject>
		<SMObject>
		  <Comment><![CDATA[Status Word 2]]></Comment>
		  <Address>#x32030010</Address>
		</SMObject>
		<SMObject>
		  <Comment><![CDATA[Position actual value]]></Comment>
		  <Address>#x60640020</Address>
		</SMObject>
		<SMObject>
		  <Comment><![CDATA[Velocity actual value]]></Comment>
		  <Address>#x606C0020</Address>
		</SMObject>
	</TxPdo>

	<DistribuitedClock>1</DistribuitedClock>
	
    <CyclicOperation>
		<Enabled>1</Enabled>

		<Sync0>1</Sync0>
		<Sync0CycleTime AdaptCycleTime="1">2000</Sync0CycleTime>
		<Sync0ShiftTime>0</Sync0ShiftTime>

		<Sync1>1</Sync1>
		<Sync1CycleTime AdaptCycleTime="1">2000</Sync1CycleTime>
		<Sync1ShiftTime>0</Sync1ShiftTime>
	</CyclicOperation>

</Config>



