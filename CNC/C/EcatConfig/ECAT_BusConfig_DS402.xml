<?xml version="1.0" encoding="iso-8859-1"?>
<!--ByStorm drive configured for Profile Velocity Mode-->
<Config DeviceConfig="el9800-ByStorm_1.30.3.xml">

	<SetUpDrive>
		<ProgramListing><![CDATA[
dim PNI as integer alias "60890001" class "SDOAccess"
dim PDI as integer alias "608A0001" class "SDOAccess"
dim VNI as integer alias "608B0001" class "SDOAccess"
dim VDI as integer alias "608C0001" class "SDOAccess"
dim ANI as integer alias "608D0001" class "SDOAccess"
dim ADI as integer alias "608E0001" class "SDOAccess"
dim ProfileAcc as integer alias "60830004" class "SDOAccess"
dim ProfileDec as integer alias "60840004" class "SDOAccess"
dim QuickStopDec as integer alias "60850004" class "SDOAccess"

PNI = -6                       'Position notation index = micron
PDI = 1                         'Position dimension index = meter
VNI = -6                       'Velocity notation index = micron
VDI = &HA6                  'Velocity dimension index = meter/sec
ANI = -6                       'Acceleration notation index = micron
ADI = &HA6                  'Acceleration dimension index = meter/sec*sec

ProfileAcc = 15000000
ProfileDec = 15000000
QuickStopDec = 500000

		]]></ProgramListing>
	</SetUpDrive>

    <Comment><![CDATA[FMMU and Sync Managment tables]]></Comment>
	<FMMUTable LStart="#x12000" Length="0" LStartBit="0" LEndBit="7" PStart="#x1800">0</FMMUTable>
	<FMMUTable LStart="#x11000" Length="0" LStartBit="0" LEndBit="7" PStart="#x1C00">1</FMMUTable>
	<FMMUTable LStart="#x400000" Length="1" LStartBit="0" LEndBit="0" PStart="#x080D">2</FMMUTable>
	<FMMUTable LStart="#x0" Length="0" LStartBit="0" LEndBit="0" PStart="#x0">3</FMMUTable>

	<SYNCTable Start="#x1000" Length="128" Type="MBoxOutput" Data="#x26000100">0</SYNCTable>
	<SYNCTable Start="#x1400" Length="128" Type="MBoxInput" Data="#x22000100">1</SYNCTable>
	<SYNCTable Start="#x1800" Length="0" Type="Output" Data="#x64000100">2</SYNCTable>
	<SYNCTable Start="#x1C00" Length="0" Type="Input" Data="#x20000100">3</SYNCTable>
    <Comment><![CDATA[FMMU and Sync Managment tables]]></Comment>

    <MboxPolling>
      <Comment><![CDATA[0=Cyclic 1=State Change]]></Comment>
      <Type>1</Type>
    </MboxPolling>
    
	<DownloadPDOAssignment>0</DownloadPDOAssignment>
    <DownloadPDOConfiguration>0</DownloadPDOConfiguration>

	<PDOAssignmentSM2 Output="#x1600">
		<SMObject>
		  <Comment><![CDATA[Ctrl Word]]></Comment>
		  <Address>#x60400010</Address>
		</SMObject>
		<SMObject>
		  <Comment><![CDATA[Ctrl Word 2]]></Comment>
		  <Address>#x300F0010</Address>
		</SMObject>
		<SMObject>
		  <Comment><![CDATA[Target Position]]></Comment>
		  <Address>#x607A0020</Address>
		</SMObject>
		<SMObject>
		  <Comment><![CDATA[Target Velocity]]></Comment>
		  <Address>#x60FF0020</Address>
		</SMObject>
	</PDOAssignmentSM2>
	
	<PDOAssignmentSM3 Input="#x1A00">
		<SMObject>
		  <Comment><![CDATA[Status Word]]></Comment>
		  <Address>#x60410010</Address>
		</SMObject>
		<SMObject>
		  <Comment><![CDATA[Status Word 2]]></Comment>
		  <Address>#x32030010</Address>
		</SMObject>
		<SMObject>
		  <Comment><![CDATA[Position actual value]]></Comment>
		  <Address>#x60640020</Address>
		</SMObject>
		<SMObject>
		  <Comment><![CDATA[Velocity actual value]]></Comment>
		  <Address>#x606C0020</Address>
		</SMObject>
	</PDOAssignmentSM3>

	<DistribuitedClock>1</DistribuitedClock>

	<CyclicOperation>
		<Enabled>1</Enabled>

		<Sync0>1</Sync0>
		<Sync0CycleTime AdaptCycleTime="1">2000</Sync0CycleTime>
		<Sync0ShiftTime>0</Sync0ShiftTime>

		<Sync1>1</Sync1>
		<Sync1CycleTime AdaptCycleTime="1">2000</Sync1CycleTime>
		<Sync1ShiftTime>0</Sync1ShiftTime>
	</CyclicOperation>

</Config>



