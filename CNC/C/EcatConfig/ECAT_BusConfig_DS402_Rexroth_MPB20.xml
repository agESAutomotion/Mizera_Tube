<?xml version="1.0" encoding="iso-8859-1"?>
<!--ByStorm drive configured for Profile Velocity Mode-->
<Config DeviceConfig="BoschRexroth_IndraDrive_CoE_01V01.xml">

	<SetUpDrive>
		<ProgramListing><![CDATA[
		]]></ProgramListing>
	</SetUpDrive>

    <Comment><![CDATA[FMMU and Sync Managment tables]]></Comment>
	<FMMUTable LStart="#x12000" Length="0" LStartBit="0" LEndBit="7" PStart="#x1000">0</FMMUTable>
	<FMMUTable LStart="#x11000" Length="0" LStartBit="0" LEndBit="7" PStart="#x1100">1</FMMUTable>
	<FMMUTable LStart="#x400000" Length="1" LStartBit="0" LEndBit="0" PStart="#x080D">2</FMMUTable>
	<FMMUTable LStart="#x0" Length="0" LStartBit="0" LEndBit="0" PStart="#x0">3</FMMUTable>

	<SYNCTable Start="#x1800" Length="234" Type="MBoxOutput" Data="#x26000100">0</SYNCTable>
	<SYNCTable Start="#x1A00" Length="234" Type="MBoxInput" Data="#x22000100">1</SYNCTable>
	<SYNCTable Start="#x1000" Length="0" Type="Output" Data="#x64000100">2</SYNCTable>
	<SYNCTable Start="#x1100" Length="0" Type="Input" Data="#x20000100">3</SYNCTable>
    <Comment><![CDATA[FMMU and Sync Managment tables]]></Comment>

    <MboxPolling>
      <Comment><![CDATA[0=Cyclic 1=State Change]]></Comment>
      <Type>1</Type>
    </MboxPolling>
    
	<DownloadPDOAssignment>1</DownloadPDOAssignment>
    <DownloadPDOConfiguration>1</DownloadPDOConfiguration>

	<PDOAssignmentSM2 Output="#x1600">
		<SMObject>
		  <Comment><![CDATA[Ctrl Word]]></Comment>
		  <Address>#x60400010</Address>
		</SMObject>
		<SMObject>
		  <Comment><![CDATA[Target Position]]></Comment>
		  <Address>#x607A0020</Address>
		</SMObject>
	</PDOAssignmentSM2>
	
	<PDOAssignmentSM3 Input="#x1A00">
		<SMObject>
		  <Comment><![CDATA[Status Word]]></Comment>
		  <Address>#x60410010</Address>
		</SMObject>
		<SMObject>
		  <Comment><![CDATA[Position actual value]]></Comment>
		  <Address>#x60640020</Address>
		</SMObject>
		<SMObject>
		  <Comment><![CDATA[Torque actual value]]></Comment>
		  <Address>#x60770010</Address>
		</SMObject>
	</PDOAssignmentSM3>

	<DistribuitedClock>1</DistribuitedClock>
	
	<CyclicOperation>
		<Enabled>1</Enabled>

		<Sync0>1</Sync0>
		<Sync0CycleTime AdaptCycleTime="1">2000</Sync0CycleTime>
		<Sync0ShiftTime>0</Sync0ShiftTime>

		<Sync1>1</Sync1>
		<Sync1CycleTime AdaptCycleTime="1">2000</Sync1CycleTime>
		<Sync1ShiftTime>500</Sync1ShiftTime>
	</CyclicOperation>

</Config>



