[PROMPT]
KEY_TWI       =Daten Spindeln

TWI_KINELIST =0-Nullo, 1=Rotary Hd, 2=Fork Hd, 3=Nutator
TWI_KINE      =Getriebetyp
TWI_DSC       =Description

RH_X0         =Koordinate X erstes Pivot
RH_Y0         =Koordinate Y erstes Pivot
RH_Z0         =Koordinate Z erstes Pivot
RH_ORIENT     =Ausrichtung Z-Achse Kopf
RH_FRAME_C    =Reference frame Z (Y) rotation
RH_FRAME_B    =Reference frame Y (P) rotation
RH_FRAME_A    =Reference frame X (R) rotation
RH_ORIENTLST  =6: X+, 8: Y+, 9: Z+
RH_EC         =Rotation um Z-Achse
RH_ECREV      =Inversion zu Rotationssteller
RH_ECAX       =Steller Rotation um Z-Achse
RH_AXLST      =0-NC, 7-A axis, 8-B axis, 9-C axis, 10-Spindle, 127-Pursuit

FRH_X0        =Koordinate X erstes Pivot
FRH_Y0        =Koordinate Y erstes Pivot
FRH_Z0        =Koordinate Z erstes Pivot
FRH_ORIENT    =Ausrichtung Z-Achse Kopf
